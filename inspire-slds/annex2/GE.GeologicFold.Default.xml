        <se:Rule>
        <se:Description>
          <se:Title>geologic fold: anticline</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>ge:GeologicFold</ogc:PropertyName>
              <ogc:Literal>anticline</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
