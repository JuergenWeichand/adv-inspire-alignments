<sld:StyledLayerDescriptor xmlns:sld="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:se="http://www.opengis.net/se" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.1.0" xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.1.0/StyledLayerDescriptor.xsd">
  <sld:NamedLayer>
    <se:Name>GE.Aquitard.ConstitutionOfAquitard</se:Name>
    <sld:UserStyle>
      <se:Name>GE.Aquitard.ConstitutionOfAquitard.Default</se:Name>
      <sld:IsDefault>1</sld:IsDefault>
      <se:FeatureTypeStyle version="1.1.0" xmlns:ge="http://inspire.ec.europa.eu/schemas/ge/4.0">
        <se:Description>
          <se:Title>Aquitard Constitution Of Aquitard Default Style</se:Title>
          <se:Abstract>The polygons (MappedFeatures) of spatial objects whose specification property is of type Aquitard.</se:Abstract>
        </se:Description>
        <se:FeatureTypeName>ge:ConstitutionOfAquitard</se:FeatureTypeName>

        <se:Rule>
        <se:Description>
          <se:Title>constitution of aquitard</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>ge:ConstitutionOfAquitard</ogc:PropertyName>
              <ogc:Literal>Aquitard</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:PolygonSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>ge:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Fill>
              <se:SvgParameter name="fill">#8CB4E2</se:SvgParameter>
            </se:Fill>
          </se:PolygonSymbolizer>
        </se:Rule>
        
      </se:FeatureTypeStyle>
    </sld:UserStyle>
  </sld:NamedLayer>
</sld:StyledLayerDescriptor>