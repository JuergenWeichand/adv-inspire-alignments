<!-- FBurkamp: Kontent und Abstract der SLD nach GE-Specification ergÃ¤nzt. (2017-06-27 18:04) -->
<sld:StyledLayerDescriptor xmlns:sld="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:se="http://www.opengis.net/se" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.1.0" xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.1.0/StyledLayerDescriptor.xsd">
  <sld:NamedLayer>
    <se:Name>GE.GeophProfile.FlightLine</se:Name>
    <sld:UserStyle>
      <se:Name>GE.GeophProfile.FlightLine.Default</se:Name>
      <sld:IsDefault>1</sld:IsDefault>
      <se:FeatureTypeStyle version="1.1.0" xmlns:ge="http://inspire.ec.europa.eu/schemas/ge/4.0">
        <se:Description>
          <se:Title>Flight Line Default Style</se:Title>
          <se:Abstract>The symbols of GeophProfile are portrayed by type (ProfileType). Note: One layer shall be made available for each code list value, for example: GE.boreholeLogging, GE.seismicLine, GE.multielectrodeDCProfile, etc.</se:Abstract>
        </se:Description>
        <se:FeatureTypeName>ge:GeophProfile.FlightLine</se:FeatureTypeName>

        <se:Rule>
        <se:Description>
          <se:Title>flight line</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>ge:GeophProfile</ogc:PropertyName>
              <ogc:Literal>FlightLine</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:LineSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>ge:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Stroke>
              <se:SvgParameter name="stroke">#3366FF</se:SvgParameter>
              <se:SvgParameter name="stroke-width">1</se:SvgParameter>
            </se:Stroke>
          </se:LineSymbolizer>
        </se:Rule>
        
      </se:FeatureTypeStyle>
    </sld:UserStyle>
  </sld:NamedLayer>
</sld:StyledLayerDescriptor>