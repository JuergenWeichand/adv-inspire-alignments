<sld:StyledLayerDescriptor xmlns:sld="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:se="http://www.opengis.net/se" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.1.0" xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.1.0/StyledLayerDescriptor.xsd">
  <sld:NamedLayer>
    <se:Name>SD.SpeciesDistributionAlternative</se:Name>
    <sld:UserStyle>
      <se:Name>SD.SpeciesDistributionAlternative.Default</se:Name>
      <sld:IsDefault>1</sld:IsDefault>
      <se:FeatureTypeStyle version="1.1.0" xmlns:sd="http://inspire.ec.europa.eu/schemas/sd/4.0">
        <se:Description>
          <se:Title>Species Distribution Default</se:Title>
          <se:Abstract>This layer style is for representing presence information of species occurrences in grid cells or irregular shaped areas as analytical aggregation units without any further differentiation of additional attributes.</se:Abstract>
        </se:Description>
        <se:FeatureTypeName>sd:SpeciesDistributionAlternative</se:FeatureTypeName>
        
        <se:Rule>
        <se:Description>
          <se:Title>species distribution: polygon with hatched lines</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>sd:SpeciesDistributionAlternative</ogc:PropertyName>
              <ogc:Literal>Polygon</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:PolygonSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>so:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Fill>
              <se:SvgParameter name="fill">#808080</se:SvgParameter>
            </se:Fill>
            <se:Stroke>
              <se:SvgParameter name="stroke">#000000</se:SvgParameter>
              <se:SvgParameter name="stroke-width">1</se:SvgParameter>
              <se:SvgParameter name="stroke-dasharray">10 10</se:SvgParameter>
            </se:Stroke>
          </se:PolygonSymbolizer>
        </se:Rule>
        
       </se:FeatureTypeStyle>
    </sld:UserStyle>
  </sld:NamedLayer>
</sld:StyledLayerDescriptor>