<sld:StyledLayerDescriptor xmlns:sld="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:se="http://www.opengis.net/se" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.1.0" xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.1.0/StyledLayerDescriptor.xsd">
  <sld:NamedLayer>
    <se:Name>SU.StatisticalGridCell</se:Name>
    <sld:UserStyle>
      <se:Name>SU.StatisticalGridCell.Default</se:Name>
      <sld:IsDefault>1</sld:IsDefault>
      <se:FeatureTypeStyle version="1.1.0" xmlns:su="http://inspire.ec.europa.eu/schemas/su/4.0">
        <se:Description>
          <se:Title>Statistical Grid Cell Default Style</se:Title>
          <se:Abstract>Polygon geometries are rendered using a 80% limegreen (#E4FF00) fill.</se:Abstract>
        </se:Description>
        <se:FeatureTypeName>su:StatisticalGridCell</se:FeatureTypeName>
        <se:Rule>
        <se:Description>
          <se:Title>statistical grid cell</se:Title>
        </se:Description>
          <se:PolygonSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>su:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Fill>
              <se:SvgParameter name="fill">#e4ff00</se:SvgParameter>
              <se:SvgParameter name="fill-opacity">0.2</se:SvgParameter>
            </se:Fill>
          </se:PolygonSymbolizer>
        </se:Rule>
      </se:FeatureTypeStyle>
    </sld:UserStyle>
  </sld:NamedLayer>
</sld:StyledLayerDescriptor>
