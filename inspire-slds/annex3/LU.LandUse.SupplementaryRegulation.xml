<!-- FBurkamp: Kontent und Abstract der SLD nach LU-Specification ergÃ¤nzt. (2017-06-21 18:04) -->
<sld:StyledLayerDescriptor xmlns:sld="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:se="http://www.opengis.net/se" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.1.0" xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.1.0/StyledLayerDescriptor.xsd">
  <sld:NamedLayer>
    <se:Name>LU.LandUse.SupplementaryRegulation</se:Name>
    <sld:UserStyle>
      <se:Name>LU.LandUse.SupplementaryRegulation.Default</se:Name>
      <sld:IsDefault>1</sld:IsDefault>
      <se:FeatureTypeStyle version="1.1.0" xmlns:lu="http://inspire.ec.europa.eu/schemas/lu/4.0">
        <se:Description>
          <se:Title>Supplementary Regulation Default Style</se:Title>
          <se:Abstract>Contour or line of the Supplementary regulation depicted by a colour line of two pixels, colour depending of the value of the attribute supplementaryRegulation Symbol for point supplementary regulation. Supplementary Regulation geometries are rendered with a colour depending on the value of the attribute from HSRCL level 1 (1_ImpactOnEnvironment: green #78A050, 2_RiskExposure: black-blue #464664, 3_HeritageProtection: brown #78503C, 4_GeneralInterest: purple #7850B4, 5_LandPropertyRight: dark red #B40032, 6_RegulationsOnBuildings: salmon #FF5050, 7_LocalRegionalStateDevelopmentPolicies: limegreen #D2DC14, 8_SocialHealthChoices: mint #50FFC8, 9_RegulatedActivities: pink #DC3Cf0, 10_OtherSupplementaryRegulation: purple-red #8C145A) lines with a stroke width of 2 pixels.</se:Abstract>
        </se:Description>
        <se:FeatureTypeName>lu:SupplementaryRegulation</se:FeatureTypeName>
        <!-- HSRCL level 1 -->
        <se:Rule>
        <se:Description>
          <se:Title>supplementary regulation: 1. impact on environment</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>lu:SupplementaryRegulation</ogc:PropertyName>
              <ogc:Literal>1_ImpactOnEnvironment</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:LineSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>lu:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Stroke>
              <se:SvgParameter name="stroke">#78A050</se:SvgParameter>
              <se:SvgParameter name="stroke-width">2</se:SvgParameter>
            </se:Stroke>
          </se:LineSymbolizer>
        </se:Rule>

        <se:Rule>
        <se:Description>
          <se:Title>supplementary regulation: 2. risk exposure</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>lu:SupplementaryRegulation</ogc:PropertyName>
              <ogc:Literal>2_RiskExposure</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:LineSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>lu:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Stroke>
              <se:SvgParameter name="stroke">#464664</se:SvgParameter>
              <se:SvgParameter name="stroke-width">2</se:SvgParameter>
            </se:Stroke>
          </se:LineSymbolizer>
        </se:Rule>
        
        <se:Rule>
        <se:Description>
          <se:Title>supplementary regulation: 3. heritage protection</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>lu:SupplementaryRegulation</ogc:PropertyName>
              <ogc:Literal>3_HeritageProtection</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:LineSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>lu:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Stroke>
              <se:SvgParameter name="stroke">#78503C</se:SvgParameter>
              <se:SvgParameter name="stroke-width">2</se:SvgParameter>
            </se:Stroke>
          </se:LineSymbolizer>
        </se:Rule> 

        <se:Rule>
        <se:Description>
          <se:Title>supplementary regulation: 4. general interest</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>lu:SupplementaryRegulation</ogc:PropertyName>
              <ogc:Literal>4_GeneralInterest</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:LineSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>lu:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Stroke>
              <se:SvgParameter name="stroke">#7850B4</se:SvgParameter>
              <se:SvgParameter name="stroke-width">2</se:SvgParameter>
            </se:Stroke>
          </se:LineSymbolizer>
        </se:Rule>         
       
        <se:Rule>
        <se:Description>
          <se:Title>supplementary regulation: 5. land property right</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>lu:SupplementaryRegulation</ogc:PropertyName>
              <ogc:Literal>5_LandPropertyRight</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:LineSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>lu:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Stroke>
              <se:SvgParameter name="stroke">#B40032</se:SvgParameter>
              <se:SvgParameter name="stroke-width">2</se:SvgParameter>
            </se:Stroke>
          </se:LineSymbolizer>
        </se:Rule> 
        
        <se:Rule>
        <se:Description>
          <se:Title>supplementary regulation: 6. regulations on buildings</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>lu:SupplementaryRegulation</ogc:PropertyName>
              <ogc:Literal>6_RegulationsOnBuildings</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:LineSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>lu:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Stroke>
              <se:SvgParameter name="stroke">#FF5050</se:SvgParameter>
              <se:SvgParameter name="stroke-width">2</se:SvgParameter>
            </se:Stroke>
          </se:LineSymbolizer>
        </se:Rule> 
        
        <se:Rule>
        <se:Description>
          <se:Title>supplementary regulation: 7. local regional state development policies</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>lu:SupplementaryRegulation</ogc:PropertyName>
              <ogc:Literal>7_LocalRegionalStateDevelopmentPolicies</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:LineSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>lu:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Stroke>
              <se:SvgParameter name="stroke">#D2DC14</se:SvgParameter>
              <se:SvgParameter name="stroke-width">2</se:SvgParameter>
            </se:Stroke>
          </se:LineSymbolizer>
        </se:Rule> 
        
        <se:Rule>
        <se:Description>
          <se:Title>supplementary regulation: 8. social health choices</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>lu:SupplementaryRegulation</ogc:PropertyName>
              <ogc:Literal>8_SocialHealthChoices</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:LineSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>lu:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Stroke>
              <se:SvgParameter name="stroke">#50FFC8</se:SvgParameter>
              <se:SvgParameter name="stroke-width">2</se:SvgParameter>
            </se:Stroke>
          </se:LineSymbolizer>
        </se:Rule> 
        
        <se:Rule>
        <se:Description>
          <se:Title>supplementary regulation: 9. regulated activities</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>lu:SupplementaryRegulation</ogc:PropertyName>
              <ogc:Literal>9_RegulatedActivities</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:LineSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>lu:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Stroke>
              <se:SvgParameter name="stroke">#DC3CF0</se:SvgParameter>
              <se:SvgParameter name="stroke-width">2</se:SvgParameter>
            </se:Stroke>
          </se:LineSymbolizer>
        </se:Rule> 
        
        <se:Rule>
        <se:Description>
          <se:Title>supplementary regulation: 10. other supplementary regulation</se:Title>
        </se:Description>
          <ogc:Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>lu:SupplementaryRegulation</ogc:PropertyName>
              <ogc:Literal>10_OtherSupplementaryRegulation</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </ogc:Filter>
          <se:LineSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>lu:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Stroke>
              <se:SvgParameter name="stroke">#8C145A</se:SvgParameter>
              <se:SvgParameter name="stroke-width">2</se:SvgParameter>
            </se:Stroke>
          </se:LineSymbolizer>
        </se:Rule> 
        
        </se:FeatureTypeStyle>
    </sld:UserStyle>
  </sld:NamedLayer>
</sld:StyledLayerDescriptor>