<sld:StyledLayerDescriptor version="1.1.0" xsi:schemaLocation="http://www.opengis.net/sld http://schemas.opengis.net/sld/1.1.0/StyledLayerDescriptor.xsd" xmlns:sld="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:se="http://www.opengis.net/se" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <sld:NamedLayer>
    <se:Name>AF.Plot.Default</se:Name>
    <sld:UserStyle>
      <se:Name>AF.Plot.Default</se:Name>
        <sld:IsDefault>1</sld:IsDefault>
        <se:FeatureTypeStyle version="1.1.0" xmlns:af="http://inspire.ec.europa.eu/schemas/af/4.0">
          <se:Description>
            <se:Title>af:Plot</se:Title>
            <se:Abstract>The geometry is rendered using a grey (#808080) fill and a solid Yellow (#FFFF00) outline with a stroke width of 1 pixel.</se:Abstract>
          </se:Description>
            <se:FeatureTypeName>af:Plot</se:FeatureTypeName>
          <se:Rule>
          <se:Description>
            <se:Title>plot</se:Title>
          </se:Description>
          <se:MinScaleDenominator>5000</se:MinScaleDenominator>
          <se:MaxScaleDenominator>25000</se:MaxScaleDenominator>
          <se:PolygonSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>ps:geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Fill>
              <se:SvgParameter name="fill">#808080</se:SvgParameter>
            </se:Fill>
            <se:Stroke>
              <se:SvgParameter name="stroke">#FFFF00</se:SvgParameter>
              <se:SvgParameter name="stroke-width">1</se:SvgParameter>
            </se:Stroke>
          </se:PolygonSymbolizer>
        </se:Rule>
        </se:FeatureTypeStyle>
    </sld:UserStyle>
  </sld:NamedLayer>
</sld:StyledLayerDescriptor>